{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "build/",
        "removeComments": false,
        "alwaysStrict": true
    },
"typedocOptions": {
        "entryPoints": ["src/index.ts","src/ants.ts","src/game.ts","src/ui.ts"],
        "out": "docs"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]

}
